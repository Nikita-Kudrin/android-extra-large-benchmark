default-scenarios = ["androidStudioSync"]

androidStudioSync {
    title = "Android Studio Sync"
    versions = [ "7.5.1" ]
    warm-ups = 1
    iterations = 1
    android-studio-sync {
        # Override default Android Studio jvm args,
        # The max memory will need adjusting
        studio-jvm-args = ["-Xms256m", "-Xmx4096m",]
    }
}